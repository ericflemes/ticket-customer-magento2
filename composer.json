{
    "name": "ticket-magento2/ticket-customer-magento2",
    "license": "GPL-3.0-only",
    "version": "1.0.8",
    "type": "magento2-module",
    "description": "Magento module to manger a ticket with customer",
    "require": {
        "php": "~7.0.0|~7.1.0",
        "paypal/rest-api-sdk-php": "^1.13.0",
        "guzzle/http": "^3.9"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.0",
        "squizlabs/php_codesniffer": "2.7.0",
        "phpmd/phpmd": "@stable",
        "pdepend/pdepend": "2.2.2",
        "sjparkinson/static-review": "~4.1",
        "fabpot/php-cs-fixer": "~1.2",
        "lusitanian/oauth": "~0.3 <=0.7.0",
        "phpunit/phpunit-selenium": ">=1.2",
        "sebastian/phpcpd": "^2.0",
        "satooshi/php-coveralls": "^1.0",
        "phpdocumentor/phpdocumentor": "^2.8",
        "magento-ecg/coding-standard": "dev-master",
        "rregeer/phpunit-coverage-check": "^0.1.3"
    },
    "authors": [
        {
            "name": "Eric Lemes",
            "email": "eric_efl@hotmail.com"
        }
    ],
    "minimum-stability": "dev",
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "Os\\Customer\\": ""
        }
    },
    "config": {
        "secure-http": false
    }
}